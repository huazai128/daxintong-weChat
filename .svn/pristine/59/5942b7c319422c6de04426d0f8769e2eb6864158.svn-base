@import 'entry';

.payment-box{
	border-top: 1px solid #f0f0f0;
	background: #fff;
}
.shop-detail{
	padding: 20px;
	width: 100%;
	overflow: hidden;
	border-bottom: 1px solid #ddd;
	.shop-img{
		width: 160px;
		height: 110px;
		overflow: hidden;
		float: left;
		> img {
			width: 100%;
			height: 100%;
		}
	}
	.shop-le{
		margin-left: 180px;
	}
	h4{
	  background: url("images/arrow_down.svg") no-repeat right center;
		background-size: 0.22rem  0.3rem;
		height: 66px;
		overflow: hidden;
		padding-right: 40px;
		padding-top: 10px;
		overflow : hidden;
		text-overflow: ellipsis;
		display: -webkit-box;
		-webkit-line-clamp: 2;
		-webkit-box-orient: vertical;
	}
	h4,	p{
		margin: 0;
		font-size: 30px;
		font-weight: normal;
		color: #333;
	}
	p{
		padding-top: 10px
	}
	.shop-type{
		font-size: 28px;
		color: #73b41c;
		line-height: 1.6;
		padding-left: 35px;
		background: url("images/select.svg") no-repeat left center;
		background-size: 32px;
		overflow: hidden;
		text-overflow: ellipsis;
		white-space: nowrap;
	}
}
.ordercenter{
	padding-left:10px;
  span{
		display: inline-block;
		font-size: 26px;
		margin-right: 30px;
		height: 30px;
	};
};
.day{
  color: #b5b5b5;
  font-size:20px;
  padding:20px 0 16px 0;
};
.dayone{
  @include bgImg(30px, 30px, 'images/gou.svg');
};
.rightone{
display: inline-block;
@include bgImg(30px, 30px, 'images/youjiantou.svg')
};
.dizong{
	font-size: 24px;
	color: #7dbd33;
};
.daytwo{
	@include bgImg(30px, 30px, 'images/gou.svg');
};
.suitui{
	font-size: 24px;
	color: #7dbd33;
	padding-bottom: 14px;
};
.erwei{
	height: 468px;
	width: 100%;
	border-bottom: 1px solid #f0f0f0;
	background: #fff
};
.erweione{
	margin-top: 30px;
};
.sao{
	height: 40px;
	width: 100%;
	font-size: 26px;
	color: #7e7e7e;
	text-align: center;
};
.imgone{
	width: 250px;
	height: 250px;
	padding-bottom: 68px;
	margin: 0 auto;
	text-align: center;
};
.yanone{
	height: 50px;
	color: #7e7e7e;
	text-align: center;
	font-size: 26px;
};
.number{
	font-size: 26px;
	color: #4995fe;
	text-align: center;
};
.shangjia{
	height: 76px;
	color: #d4d4d4;
	font-size: 26px;
	padding-left: 24px;
	border-bottom: 1px solid #e4e4e4;
	line-height: 76px;
};
// 海归
.haitop{
	font-size: 30px;
	font-weight: 600;
	margin:0;
};
.haigui{
	width: 100%;
	padding-left: 16px;
	border-bottom: 2px solid #d8d8d8;
	padding: 20px 0;
}

.rightphone{
	width: 100px;
	margin-right: 14px;
	background: url("images/phone-blue.svg") no-repeat center;
	background-size: 46px;
	position: relative;
	&::before{
		position: absolute;
		top: 10px;
		bottom: 10px;
		left: 0;
		width: 1px;
		background: #ddd;
		content: "";
	}
}
.lefthai{
	border-right: 1px solid #999999;
	padding:0 20px;
};
.dingweifont{
	font-size: 24px;
	background: url("images/position01.svg") no-repeat left center;
	background-size: 30px;
	color: #999999;
	height: 30px;
	padding-left: 36px;
};
.address{
	font-size: 24px;
	color: #999999;
	margin: 10px 0;
	// margin-bottom: 20px;
};
.bg{
	height: 22px;
	background-color: #f4f4f4;
	border-bottom: 1px solid #d8d8d8;
};
// 订单信息
.messagetop{
	padding:20px 0 30px 20px;
};
.messageorder{
	font-size: 26px;
	color: #999999;
	padding-bottom: 30px;
};
.team-bg{
	background: #fff;
}
:global{
	.t-icon{
		font-size: 26px;
		color: #333;
		padding:10px 0 10px 45px;
		&.icon-one{
			background: url("~images/number.svg") no-repeat left center;
			background-size: 32px;
		}
		&.icon-two{
			background: url("~images/time01.svg") no-repeat left center;
			background-size: 28px;
		}
		&.icon-three{
			background: url("~images/shuliang.svg") no-repeat left center;
			background-size: 36px;
		}
		&.icon-fore{
			background: url("~images/money.svg") no-repeat left center;
			background-size: 28px;
		}

	};
	.cate-share{
		width: 100%;
		height: 80px;
		position: relative;
		&::before{
			position: absolute;
			top: 0;
			left: 0;
			right: 20px;
			bottom: 0;
			content: '';
			background: url('~images/share01.svg') no-repeat right center;
			background-size: 45px ;
		}
		&.active:before{
			background: url('~images/share-yes.svg') no-repeat right center;
			background-size: 45px ;
		}
	}
	.tags-box{
		height: 44px;
		overflow: hidden;
	}
}
