@import 'entry';

.cd-bg{
	background: #fff;
}
.taungou{
	width: 100%;
	height: 100%;
	overflow: hidden;
	background-color:#f4f4f4;
	.swiper-box{
		width: 100%;
		height: 360px;
		position: relative;
		overflow: hidden;
		background-color: #fff;
		a {
			display: block;
			width: 100%;
			height: 360px;
			overflow: hidden;
		}
		img{
			width: 100%;
			height: 100%;
		}
		.swper-name{
			position: absolute;
			bottom:0;
			left: 0;
			width: 100%;
			height: 80px;
			background: rgba(0,0,0,0.2);
			font-size: 28px;
			> p{
				padding: 0 20px;
				line-height: 80px;
				color: #fff;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
				font-weight: bold;
			}
		}
	}

	li{
		list-style: none;

	};
	.leftyuan{
		font-size: 30px;
		color: #18a2fa;
	};
	.leftone{
		padding-top: 8px;
		font-size: 50px;
		color: #18a2fa;
	};
	.centertwo{
		padding-top: 15px;
		font-size: 24px;
		color: #999999;
	};
	.rightthree{
		background-color: #ff9e05;
		width: 200px;
		height: 80px;
		text-align: center;
		line-height: 80px;
		border-radius: 10px;
		margin-right: 26px;
		margin-top: 20px;
		color: #ffffff;
		font-size: 28px;
	};
	.ulone{
		border-bottom: 2px solid #e5e5e5;
		height: 128px;
		line-height: 128px;
		background: #fff;
		margin: 0;
		margin-bottom: 20px;
	};
	// 随时退货
	.tui-db{
		background: #fff;
		border-bottom: 2px solid #e5e5e5;
		padding: 20px 20px 5px;
		.tui-detail{
			padding-bottom: 15px;
			> p {
				font-size: 26px;
				margin-bottom: 5px;
				color: #6bbb00;
			}
		}
	}
	.suitui{
		display: inline-block;
		margin-right: 14px;
		@include bgImg(34px, 34px, 'images/select.svg');
	};
	.zidong{
		display: inline-block;
		margin-right: 14px;
		@include bgImg(34px, 34px, 'images/select.svg');
	};
	.yishou{
		display: inline-block;
		margin-right: 14px;
		background-image: url("images/me.svg");
		background-size: 32px 30px;
		background-repeat: no-repeat;
		width: 36px;
		height: 36px;
		background-position: center;
	};
	.day{
		display: inline-block;
		margin-right: 14px;
		background-image: url("images/clock.svg");
		background-size: 32px 32px;
		background-repeat: no-repeat;
		width: 32px;
		height: 32px;
		background-position: center;
	};
	.xing{
		background-color: #ffffff;
		border-bottom: 2px solid #e5e5e5;
		padding: 20px;
		.xingcenter{
			font-size: 28px;
			padding-right: 36px;
			background: url('images/youjiantou.svg') no-repeat right center;
			background-size: 30px;
		}
	};
	.leftxing{
		display: inline-block;
		margin-right: 14px;
		@include bgImg(35px, 35px, 'images/xing.svg');
	};

	// 商家信息
	.message{
		height: 80px;
		line-height: 80px;
		color: #999999;
		font-size: 30px;
		background: #fff;
		padding: 0 20px;
		border-bottom: 1px solid #e5e5e5;
	};
	// 海归
	.haitop{
		font-size: 30px;
		font-weight: 600;
		margin:0;
	};
	.haigui{
		padding: 20px;
		width: 100%;
		padding-left: 16px;
		background: #fff;
		border-bottom: 2px solid #e5e5e5;
		h3{
		}
	}

	.rightphone{
		width: 90px;
		background: url('images/phone-blue.svg') no-repeat right center;
		background-size: 44px 48px;
	}
	.lefthai{
		border-right: 1px solid #999999;
		padding-right: 20px;
		> h3{
			font-size: 30px;
			color: #333;
			font-weight: normal;
			overflow: hidden;
			text-overflow: ellipsis;
			white-space: nowrap;
		}
		> p {
			overflow : hidden;
			text-overflow: ellipsis;
			display: -webkit-box;
			-webkit-line-clamp: 2;
			-webkit-box-orient: vertical;
			margin: 15px 0 10px;
		}
	};
	.dingweifont{
		display: block;
		font-size: 24px;
		line-height: 1.6;
		color: #999;
		padding-left: 40px;
		background: url("images/position01.svg") no-repeat left center;
		background-size: 30px 50px;

	};
	.address{
		font-size: 24px;
		color: #999999;
		margin:20px 0 20px 0;
		// margin-bottom: 20px;
	};

	.hui{
	height: 24px;
	background-color: #f4f4f4;
	border-bottom: 2px solid #e5e5e5;
	};
	.tao{
		margin:0 16px 0 16px;
		border-bottom: 2px solid #e5e5e5;
		height:84px;
		line-height: 84px;
	};
	.zhushi{
		height: 70px;
		width: 100%;
	}
	.food{
		height: 60px;
		line-height: 60px;
		background-color: #f4f5fa;
		margin:4px 16px 4px 16px;
		border-bottom: 2px solid #e5e5e5;
	};
	.list{
		margin-right: 30px;
		border-bottom: 2px solid #e5e5e5;
		height: 74px;
		line-height: 74px;
		padding-left:16px;
	};
	a{
		color: #0c0c0c;
		font-size: 24px;
	};
	.repeat{
		height: 68px;
		border-bottom: 2px solid #e5e5e5;
	};
	.repeatone{
		font-size: 24px;
		height: 64px;
		// line-height: 64px;
		background-color: #f4f5fa;
		margin:4px 0 4px 0;
	};

	.tuxiang{
		border-bottom: 2px solid #e5e5e5;
		height:72px;
		line-height: 72px;
		font-size: 24px;
		color: #ffb969;
		padding-left: 16px;
		background-color: #ffffff;
	};
	.tu{
		margin-top:20px;
		color: #ffb969;
		display: inline-block;
		@include bgImg(35px, 35px, 'images/youjiantou.svg');
	};
	.gehang{
		height: 22px;
		background-color: #f4f4f4;
	};
	.shopping{
		height: 75px;
		line-height: 75px;
		padding:0 20px;
		background: #ffffff;
		border-bottom: 1px solid #e5e5e5;
		color: #999;
		font-size: 28px;
	};
	// 是由规则
	.timeone{
		background: #fff;
		padding: 0 20px;
		> h4{
			margin:0;
			padding-top: 20px;
		}
		> p{
			color: #666;
		}
	};
	.validity{
	font-size: 25px;
	color: #ffaa3d;
	};
	.time{
		font-size: 25px;
		margin-top:24px;
		padding-left:28px;
	};
	dl{
		border-bottom: 2px solid #e5e5e5;
		margin-bottom: 20px !important;
	}
	dt{
		font-size: 25px;
		color: #ffaa3d;
		font-weight: 600;
		padding:0 0 14px 16px;
	};
	dd{
		font-size: 25px;
		padding:11px 16px;
	};
	.lastdd{
		padding-bottom: 40px;
		display: block;
	};
	.gese{
		height: 24px;
		background-color: #f4f4f4;
		border-bottom: 2px solid #e5e5e5;
	};
	.quanbu{
	font-size: 25px;
	height: 85px;
	line-height: 85px;
	padding:0 30px 0 16px;
	};
	.tu{
		display: inline-block;
		background-position: 0 3px;
	};
	.tui{
		height: 72px;
		background-color: #ffffff;
	}
	.jian{
		height: 72px;

		line-height: 72px;
		font-size: 25px;
		border-bottom: 2px solid #e5e5e5;
		margin:0 30px 0 16px;
	};
	.tupian{
		padding-bottom: 42px;
		background-color: #ffffff;
		img{
			width: 334px;
			height: 260px;
			margin:10px 18px 0 16px;
	}
	}
};
.tuan-detail{
	padding: 20px 20px 0 20px;
	border-bottom: 1px solid #f0f0f0;
	background: #fff;
	> p {
		font-size: 28px;
		color: #999;
		margin-bottom: 20px;
	}
	.shop-lists{
		.shop-list{
			h4{
				padding: 4px 0;
				border-top: 1px solid #f0f0f0;
				border-bottom: 1px solid #f0f0f0;
				text-align: center;
				line-height: 78px;
				height: 78px;
				margin: 0;
				font-size: 30px;
				> span{
					display: block;
					background: #f4f5fa;
				}
			}
			> div{
				border-bottom: 1px solid #f0f0f0;
				line-height: 78px;
				height: 78px;
				padding: 0 10px;
				font-size: 26px;
				> div:nth-child(2){
					text-align: center;
				}
				> div:last-child{
					text-align: right;
				}
				&:last-child{
					border-bottom: 0;
				}
			}
		}
	}
	.img-detail{
		border-top: 2px solid #f0f0f0;
		height: 74px;
		line-height: 74px;
		> div{
			display: block;
			color: #fda007;
			font-size: 26px;
		}
	}
}
.rule-box{
	background: #fff;
	margin-top: 0 !important;
	> dt{
		margin: 0;
		padding-top: 20px;

	}
	> dd{
		color: #666;
	}
}

.bus-comment{
	background: #fff;
	padding:0 20px;
	> h4{
		font-size: 30px;
		font-weight: normal;
		padding: 20px;
		margin: 0;
		background: url('images/arrow_down.svg') no-repeat right center;
		background-size: 22px  30px;
		span{
			font-size: 24px;
			color: #666;
		}
	}
}
.no-comments{
	text-align: center;
	line-height: 80px;
	border-top: 1px solid #ddd;
	font-size: 28px;
	color: #666;
	background: #fff;
}

.bus-btn{
	padding: 0 20px;
	height: 80px;
	line-height: 80px;
	position: relative;
	overflow: hidden;
	padding-right: 20px;
	&::before{
		position: absolute;
		top: 0;
		left: 0;
		content:"";
		width: 100%;
		height: 1px;
		background: #e4e4e4;
	}
	> div{
		padding-right: 30px;
		font-size: 24px;
		color: #999;
		background: url('images/arrow_down.svg') no-repeat right center;
		background-size: 22px  30px;
	}
	> p{
		font-size: 30px;
		color: #333;
	}
}
:global{
	.swiper-container{
		.am-carousel,.slider-frame,.slider-list{
			height: 400px !important;
		}
	}
	.cate-comments{
		background: #fff;
		padding: 0 20px;
		height: 80px;
		font-size: 28px;
		.cate-more{
			width: 250px;
			font-size: 28px;
			padding-right: 35px;
			text-align: right;
			background: url('~images/arrow_down.svg') no-repeat right center;
			background-size: 22px  30px;
		}
	}
	.share-btns{
		width: 100%;
		height: 70px;
		> div{
			width: 70px;
			height: 100%;
			&.star{
				background: url("~images/star-empty.svg") no-repeat center;
				background-size: 44px;
				&.active{
					background: url("~images/star01.svg") no-repeat center;
					background-size: 44px;
				}
			}
			&.home{
				background: url("~images/home02.svg") no-repeat center;
				background-size: 40px;
			}
		}
	}
	.more-len{
		border-top: 1px solid #ddd;
		height: 90px;
		font-size: 28px;
		color: #ff9900;
	}
	.notes-text{
		padding: 20px;
		background: #fff;
		border-bottom: 1px solid #ddd;
	}
	.package-text{
		padding-bottom: 20px;
	}
	.font-gray{
		color: #666 !important;
	}
	.no-order{
		background: #e5e5e5 !important;
		color: #666 !important;
	}
}
