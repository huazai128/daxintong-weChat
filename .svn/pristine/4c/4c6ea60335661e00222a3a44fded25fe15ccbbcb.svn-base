{
  "name": "dxt-wx",
  "version": "1.0.0",
  "description": "create by fongwell",
  "main": "index.js",
  "theme": {
    "hd": "0.02rem",
    "brand-primary": "#18a3f9"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist/*",
    "babel": "./node_modules/.bin/babel index.js",
    "nodemon": "./node_modules/.bin/nodemon --exec babel-node",
    "babel-node": "./node_modules/.bin/babel-node",
    "es-checker": "./node_modules/.bin/es-checker",
    "dll": "node -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --colors --config webpack.dll.js",
    "start": "cross-env NODE_ENV=development node -r babel-register client/server.dev.js  --progress --colors --profile",
    "dll-start": "npm run dll && npm start",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --progress --profile --colors --config webpack.config.js",
    "build": "npm run clean && npm run build-main",
    "server": "npm run nodemon server/server.js",
    "buildprod": "cross-env config_env=production webpack  --config webpack.prod.js",
    "server-api": "npm run nodemon server/api.js"
  },
