:global{
	.form-box{
		padding: 55px 20px;
		.form-group{
			margin-bottom: 20px;
			.register-send-btn__3jSAI{
				width: 250px;
				font-size: 28px;
			}
		}
		.am-list-item .am-input-control input{
			font-size: 28px;
		}
	}

	.am-list-item.am-input-item{
		height: 92px !important;
	}
	.am-list-item .am-input-label.am-input-label-5{
		width: auto ;
	}
	.am-list-item.am-input-item{
		padding-left: 20px !important;
		background: #fff;
	}
	.am-list-item{
		background: #f5f5f9 ;
	}
	.from-btn{
		width: 250px;
		height: 92px;
		margin-left: 20px;
		background: #ddd;
	}
	.form-icon{
		display: block;
		width: 70px;
		height: 92px;
	}
	.is-send{
		color: #fff !important;
		background: #ff9f00 !important;
	}
}
.btn{
	width: 100%;
	height: 90px;
	margin-top: 100px;
	line-height: 90px;
	text-align: center;
	font-size: 32px;
	color: #fff;
	border: 0;
	outline: none;
	background: #4794fe;
	border-radius: 12px;
}
.send-btn{
	width: 250px;
	font-size: 28px;
	color: #333;
	background: #ddd;
	border: 0;
	margin-left: 20px;
	outline: none;
}

.test-img{
	overflow: hidden;
	> img{
		width: 100%;
		height: 100%;
	}
}
@mixin iconImg($width,$height,$icon){
	background:url($icon) no-repeat center;
	background-size: $width $height;
}
// .accoumt{
// 	@include iconImg(50px,50px,'images/card.svg');
// }
.password{
	@include iconImg(34px,34px,'images/suo.svg');
}
.yu{
	@include iconImg(52px,52px,'images/shenfen.svg');
}
.name{
	@include iconImg(46px,46px,'images/ming.svg');
}
.phone{
	@include iconImg(55px,55px,'images/shouji1.svg');
}
.icon-ma{
	@include iconImg(44px, 49px, 'images/xiaoyanma.svg');
}
